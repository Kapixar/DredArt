{
    "name": "DredArt",
    "description": "Dredark extension for big pixelarts. Use any PNG and paint it with Holo!",
    "version": "1.0",
    "author": "I am Shrek, Discord - Kapi73#3815",
    "manifest_version": 3,
    "content_scripts": [
        {
           "matches": ["https://*.drednot.io/*"],
           "css": ["style.css"],
           "js": ["script.js"]
        }
    ], 
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "host_permissions": [
        "https://*.drednot.io/*"
    ]
}